{
    "version": 2,
    "builds": [
      {
        "src": "**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.+\\.(css|js))",
        "dest": "/$1",
        "headers": {
          "Content-Type": "$2"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "headers": {
          "Content-Type": "text/html"
        }
      }
    ]
  }